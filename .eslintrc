{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "space-before-function-paren": ["error", "always"],
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-console": 1,
    "prefer-promise-reject-errors": 0
  }
}
